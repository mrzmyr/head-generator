<html<% if(data.language) { %> lang="<%= data.language %>"<% } %>>
<head<% if(f.isOpenGraph) { %> prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb#"<% } %>>
	<% if(data.charset) { %><meta charset="<%= data.charset %>"><% } %>

	<% if(data.title) { %><title><%= data.title %></title><% } %>

	<!-- basic -->
	<% if(data.author) { %><meta name="author" content="<%= data.author %>"><% } %>
	<% if(data.keywords) { %><meta name="keywords" content="<%= data.keywords %>"><% } %>
	<% if(data.description) { %><meta name="description" content="<%= data.description %>"><% } %>
	<% if(data.robots) { %><meta name="robots" content="<%= data.robots %>"><% } %>
	<% if(data.generator) { %><meta name="generator" content="<%= data.generator %>"><% } %>
	<% if(data.base) { %><meta name="base" content="<%= data.base %>"><% } %>

	<% if(f.getViewport(data)) { %>
	<!-- viewport -->
	<meta name="viewport" content="<%= f.getViewport(data) %>">
	<% } %>

	<% if(data['app-capable']) { %><meta name="apple-mobile-web-app-capable" content="<%= data['app-capable'] %>"><% } %>
	<% if(data['status-bar-style']) { %><meta name="apple-mobile-web-app-status-bar-style" content="<%= data['status-bar-style'] %>"><% } %>
	<% if(data['format-detection']) { %><meta name="format-detection" content="<%= data['format-detection'] %>"><% } %>

	<% if(data.imagetoolbar || data.xuacompatible) { %>
	<!--[if ie]>
		<% if(data.xuacompatible) { %><meta http-equiv="X-UA-Compatible" content="<%= data.xuacompatible %>"><% } %>
		<% if(data.imagetoolbar) { %><meta http-equiv="imagetoolbar" content="<%= data.imagetoolbar %>" /><% } %>
	<![endif]-->
	<% } %>

	<% if(data['fb-app-id'] || data['og-type'] || data['og-url'] || data['og-image'] || data['og-title'] || data['og-description']) { %>
	<!-- open-graph -->
	<% if(data['fb-app-id']) { %><meta property="fb:app_id" content="<%= data['fb-app-id'] %>"><% } %>
	<% if(data['og-type']) { %><meta property="og:type" content="<%= data['og-type'] %>"><% } %>
	<% if(data['og-url']) { %><meta property="og:url" content="<%= data['og-url'] %>"><% } %>
	<% if(data['og-image']) { %><meta property="og:image" content="<%= data['og-image'] %>"><% } %>
	<% if(data['og-title']) { %><meta property="og:title" content="<%= data['og-title'] %>"><% } %>
	<% if(data['og-description']) { %><meta property="og:description" content="<%= data['og-description'] %>"><% } %>
	<% } %>

	<% if(data['ip-name'] || data['ip-description'] || data['ip-image']) { %>
	<!-- itemsprops -->
	<% if(data['ip-name']) { %><meta itemprop="name" content="title of your content"><% } %>
	<% if(data['ip-description']) { %><meta itemprop="description" content="description"><% } %>
	<% if(data['ip-image']) { %><meta itemprop="image" content="image url"><% } %>
	<% } %>

	<!-- links -->
	<% if(data.canonical) { %><link rel="canonical" href="<%= data.canonical %>"><% } %>
	<% if(data.favicon) { %><link rel="shortcut icon" href="<%= data.favicon %>"><% } %>
	
	<% if(data['app-icon-default']) { %><link rel="apple-touch-icon" href="<%= data['app-icon-default'] %>"><% } %>
	<% if(data['app-icon-ipad']) { %><link rel="apple-touch-icon" sizes="76x76" href="<%= data['app-icon-ipad'] %>"><% } %>
	<% if(data['app-icon-iphone-retina']) { %><link rel="apple-touch-icon" sizes="120x120" href="<%= data['app-icon-iphone-retina'] %>"><% } %>
	<% if(data['app-icon-ipad-retina']) { %><link rel="apple-touch-icon" sizes="152x152" href="<%= data['app-icon-ipad-retina'] %>"><% } %>


	<% if(data['apple-touch-startup-image']) { %><link rel="apple-touch-startup-image" href="<%= data['apple-touch-startup-image'] %>"><% } %>

</head>